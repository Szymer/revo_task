{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Django: runserver on 0.0.0.0:8080",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "0.0.0.0:8080"
      ],
      "django": true,
      "justMyCode": true
    },
    {
      "name": "Django: custom command download_revo_data",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "download_revo_data"
      ],
      "django": true,
      "justMyCode": true
    }
  ]
}
